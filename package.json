{
  "name": "safe-roadmap",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "scripts": {
    "api-docs": "swagger-cli bundle ./swagger/openapi.yaml --outfile ./swagger/swagger.yaml --type yaml",
    "predev": "npm run api-docs",
    "dev": "nodemon",
    "prestart": "npm run api-docs",
    "start": "node index.js",
    "build": "npx babel index.js -d dist && npx babel src -d dist/src && npx babel config -d dist/config"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/preset-env": "^7.22.9"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.1",
    "swagger-cli": "^4.0.4",
    "swagger-ui-express": "^5.0.0"
  }
}
